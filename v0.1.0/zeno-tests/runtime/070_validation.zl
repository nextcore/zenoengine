http.post: "/api/validate_test"
  $form_data: {
    email: "invalid-email"
    age: "15"
    name: "Jo"
    username: "x"
  }

  $rules: {
    email: "required|email"
    age: "required|numeric|min:18|max:100"
    name: "required|min:3"
    username: "required|min:3|max:20"
  }

  validate: $form_data
    rules: $rules
    as: $errors

  http.response: 200
    body: $errors
