// Extended Stdlib Test

// JSON
let obj = {"id": 1, "name": "Zeno"};
let json_str = json_stringify(obj);
print("JSON: " + json_str);

let parsed = json_parse(json_str);
print("Parsed Name: " + parsed["name"]);

// Security
let uuid = uuid_v4();
print("UUID Len: " + str(len(uuid))); // Should be 36

let hash = hash_sha256("hello world");
print("SHA256: " + hash);

// Random
let r = random_int(1, 100);
print("Random: " + str(r));

// Env
// Mocking env var in test might be hard without setting it in main.rs or shell
// But we can check a common one like "PATH" or "HOME" (if on unix) or just check it returns null for unknown
let path_env = env_get("PATH");
if (path_env != null) {
    print("Env PATH found");
} else {
    print("Env PATH null");
}
