// String Utils
let greeting = str_concat("Hello", " ", "World");
print(greeting); // Hello World

let replaced = str_replace(greeting, "World", "Zeno");
print(replaced); // Hello Zeno

// Array Utils
let items = ["A", "B", "C"];
let joined = arr_join(items, "-");
print(joined); // A-B-C

let popped = arr_pop(items);
print(popped); // C
print(len(items)); // 2

// File IO
let content = "Hello File!";
file_write("test_file.txt", content);
let read_back = file_read("test_file.txt");
print("File Read: " + read_back);
file_delete("test_file.txt");

// Map Utils
let user = {"name": "Zeno", "age": 1};
let keys = map_keys(user);
// Note: keys order is not guaranteed
print("Map Keys Count: " + str(len(keys)));

// Time
let now = time_now();
print("Time ISO: " + now);
