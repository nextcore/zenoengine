print("--- ZenoLang HTTP Client Test ---");

print("Fetching external data...");
// Using httpbin.org to test GET
let response = http_get("https://httpbin.org/get");
print("GET Response length: " + str(len(response)));

if (len(response) > 0) {
    print("GET request successful!");
} else {
    print("GET request failed (might be network issue)");
}

print("Posting data...");
// Using httpbin.org to test POST
let payload = "{\"name\": \"Zeno\", \"lang\": \"Rust\"}";
let post_resp = http_post("https://httpbin.org/post", payload);
print("POST Response length: " + str(len(post_resp)));

if (len(post_resp) > 0) {
    print("POST request successful!");
}
