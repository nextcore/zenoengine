print("--- ZenoLang Built-in Functions Test ---");

let text = "hello zeno";
print("Original text: " + text);

let u = upper(text);
print("Uppercase: " + u);

let l = len(text);
print("Length: " + str(l));

print("Type conversion (str):");
print(str(100) + " is a number");

fn greet(name) {
    return "Hello " + upper(name);
}

print("Combined with function:");
print(greet("world"));
