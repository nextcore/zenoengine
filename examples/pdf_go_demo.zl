log: "Generating Invoice PDF..."

$res = pdf.generate_invoice: {
    customer: "Acme Corp"
    amount: 1500.50
}

if: $res != nil {
    then: {
        log: "✅ PDF Generated Successfully!"
        // Saving to file
        io.file.write: {
            path: "invoice_b64.txt"
            content: $res.pdf_base64
        }
    }
    else: {
        log: "❌ Error: Failed to generate PDF"
    }
}
